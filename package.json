{
  "name": "xauny.com",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": ">=18"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.storybook": "storybook build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/vercel-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "vercel deploy",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "test.e2e": "playwright test",
    "test.unit": "vitest components",
    "test.unit.ui": "vitest --ui components",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/partytown": "0.10.3",
    "@builder.io/qwik": "1.12.1",
    "@builder.io/qwik-city": "1.12.1",
    "@playwright/test": "1.51.1",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-links": "8.6.14",
    "@storybook/blocks": "8.6.14",
    "@storybook/builder-vite": "8.6.14",
    "@storybook/html": "8.6.14",
    "@storybook/html-vite": "8.6.14",
    "@types/eslint": "8.56.12",
    "@types/node": "20.15.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitest/ui": "1.6.1",
    "autoprefixer": "10.4.21",
    "eslint": "9.23.0",
    "eslint-plugin-qwik": "1.12.1",
    "postcss": "8.4.41",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.12",
    "storybook": "8.6.14",
    "storybook-framework-qwik": "0.4.3",
    "tailwindcss": "4.1.10",
    "typescript": "5.4.5",
    "undici": "6.21.3",
    "vercel": "34.4.0",
    "vite": "5.4.15",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.1"
  },
  "dependencies": {
    "@modular-forms/qwik": "0.29.1",
    "@qwikest/icons": "0.0.13",
    "@unpic/qwik": "0.0.46",
    "qwik-image": "0.0.16"
  }
}
